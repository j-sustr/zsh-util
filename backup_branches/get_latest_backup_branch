__get_latest_backup_branch() {
  latest_branch=$(list_backup_branches \
    | grep -E '\-\-backup[0-9]+$' \
    | awk -F'--backup' '{print $2, $0}' \
    | sort -n \
    | tail -n 1 \
    | cut -d' ' -f2-)
  
  if [ -z "$latest_branch" ]; then
    echo ""
  else
    echo "$latest_branch"
  fi
}

__get_latest_backup_branch